{
    "name": "laltu/modular",
    "description": "Modularize your Laravel apps",
    "keywords": [
        "laravel",
        "modules",
        "modular",
        "module"
    ],
    "authors": [
        {
            "name": "Chris Morrell",
            "homepage": "http://www.cmorrell.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "composer/composer": "^2.1",
        "illuminate/support": "^11|^12"
    },
    "require-dev": {
        "orchestra/testbench": "^9.11|^10.0",
        "friendsofphp/php-cs-fixer": "^3.14",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5|^10.5|^11.5",
        "ext-json": "*",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Laltu\\Modular\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laltu\\Modular\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laltu\\Modular\\Support\\ModularServiceProvider",
                "Laltu\\Modular\\Support\\ModularizedCommandsServiceProvider",
                "Laltu\\Modular\\Support\\ModularEventServiceProvider"
            ],
            "aliases": {
                "Modules": "Laltu\\Modular\\Support\\Facades\\Modules"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
